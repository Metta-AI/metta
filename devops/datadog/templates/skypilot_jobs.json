        "width": 3,
        "height": 2
      }
    },
    {
      "definition": {
        "title": "Job Status Over Time",
        "title_size": "16",
        "title_align": "left",
        "show_legend": true,
        "legend_layout": "auto",
        "legend_columns": ["avg", "min", "max", "value"],
        "type": "timeseries",
        "requests": [
          {
            "formulas": [
              {
                "formula": "query1",
                "alias": "Running"
              },
              {
                "formula": "query2",
                "alias": "Queued"
              },
              {
                "formula": "query3",
                "alias": "Failed"
              }
            ],
            "queries": [
              {
                "data_source": "metrics",
                "name": "query1",
                "query": "avg:skypilot.jobs.running{*}"
              },
              {
                "data_source": "metrics",
                "name": "query2",
                "query": "avg:skypilot.jobs.queued{*}"
              },
              {
                "data_source": "metrics",
                "name": "query3",
                "query": "avg:skypilot.jobs.failed{*}"
              }
            ],
            "response_format": "timeseries",
            "style": {
              "palette": "dog_classic",
              "line_type": "solid",
              "line_width": "normal"
            },
            "display_type": "area"
          }
        ]
      },
      "layout": {
        "x": 0,
        "y": 2,
        "width": 6,
        "height": 3
      }
    },
    {
      "definition": {
        "title": "Job Success Rate (%)",
        "title_size": "16",
        "title_align": "left",
        "show_legend": true,
        "legend_layout": "auto",
        "legend_columns": ["avg", "min", "max", "value"],
        "type": "timeseries",
        "requests": [
          {
            "formulas": [
              {
                "formula": "100 * (query1 / (query1 + query2))",
                "alias": "Success Rate %"
              }
            ],
            "queries": [
              {
                "data_source": "metrics",
                "name": "query1",
                "query": "sum:skypilot.jobs.succeeded{*}"
              },
              {
                "data_source": "metrics",
                "name": "query2",
                "query": "sum:skypilot.jobs.failed{*}"
              }
            ],
            "response_format": "timeseries",
            "style": {
              "palette": "green",
              "line_type": "solid",
              "line_width": "normal"
            },
            "display_type": "line"
          }
        ],
        "markers": [
          {
            "label": " Target: > 95% ",
            "value": "y = 95",
            "display_type": "ok dashed"
          }
        ],
        "yaxis": {
          "min": "0",
          "max": "100",
          "include_zero": true,
          "scale": "linear"
        }
      },
      "layout": {
        "x": 6,
        "y": 2,
        "width": 6,
        "height": 3
      }
    },
    {
      "definition": {
        "title": "Runtime Distribution (Running Jobs)",
        "title_size": "16",
        "title_align": "left",
        "show_legend": true,
        "legend_layout": "auto",
        "legend_columns": ["avg", "min", "max", "value"],
        "type": "timeseries",
        "requests": [
          {
            "formulas": [
              {
                "formula": "query1",
                "alias": "0-1 hour"
              },
              {
                "formula": "query2",
                "alias": "1-4 hours"
              },
              {
                "formula": "query3",
                "alias": "4-24 hours"
              },
              {
                "formula": "query4",
                "alias": "> 24 hours"
              }
            ],
            "queries": [
              {
                "data_source": "metrics",
                "name": "query1",
                "query": "avg:skypilot.jobs.runtime_buckets.0_1h{*}"
