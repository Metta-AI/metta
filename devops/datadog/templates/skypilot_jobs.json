{
  "id": "wjp-n4n-dsf",
  "title": "Skypilot Jobs Dashboard",
  "description": "Job orchestration, resource utilization, and compute tracking from Skypilot. Monitors running jobs, clusters, GPU usage, and reliability.",
  "layout_type": "ordered",
  "url": "/dashboard/wjp-n4n-dsf/skypilot-jobs-dashboard",
  "template_variables": [],
  "widgets": [
    {
      "definition": {
        "title": "Running Jobs",
        "title_size": "16",
        "title_align": "left",
        "type": "query_value",
        "requests": [
          {
            "formulas": [
              {
                "formula": "query1"
              }
            ],
            "queries": [
              {
                "data_source": "metrics",
                "name": "query1",
                "query": "avg:skypilot.jobs.running{*}",
                "aggregator": "last"
              }
            ],
            "response_format": "scalar"
          }
        ],
        "autoscale": true,
        "precision": 0
      },
      "layout": {
        "x": 0,
        "y": 0,
        "width": 3,
        "height": 2
      }
    },
    {
      "definition": {
        "title": "Queued Jobs",
        "title_size": "16",
        "title_align": "left",
        "type": "query_value",
        "requests": [
          {
            "formulas": [
              {
                "formula": "query1"
              }
            ],
            "queries": [
              {
                "data_source": "metrics",
                "name": "query1",
                "query": "avg:skypilot.jobs.queued{*}",
                "aggregator": "last"
              }
            ],
            "response_format": "scalar"
          }
        ],
        "autoscale": true,
        "precision": 0
      },
      "layout": {
        "x": 3,
        "y": 0,
        "width": 3,
        "height": 2
      }
    },
    {
