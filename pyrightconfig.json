{
  "include": [
    "**/*.py"
  ],
  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    ".venv"
  ],
  "extraPaths": [
    "./common/src",
    "./mettagrid/src",
    "./agent/src",
    "./app_backend/src"
