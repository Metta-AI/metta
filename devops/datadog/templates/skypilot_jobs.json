              {
                "data_source": "metrics",
                "name": "query4",
                "query": "avg:skypilot.jobs.runtime_buckets.over_24h{*}"
              }
            ],
            "response_format": "timeseries",
            "style": {
              "palette": "cool",
              "line_type": "solid",
              "line_width": "normal"
            },
            "display_type": "bars"
          }
        ],
        "markers": [
          {
            "label": " Warning: Many long-running jobs ",
            "value": "y = 20",
            "display_type": "warning dashed"
          }
        ]
      },
      "layout": {
        "x": 0,
        "y": 5,
        "width": 6,
        "height": 3
      }
    },
    {
      "definition": {
