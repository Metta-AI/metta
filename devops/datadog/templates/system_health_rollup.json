      "definition": {
        "title": "Tests Passing on Main (FoM)",
        "title_size": "16",
        "title_align": "left",
        "show_legend": false,
        "type": "timeseries",
        "requests": [
          {
            "formulas": [
              {
                "formula": "query1",
                "style": {
                  "palette": "green"
                }
              }
            ],
            "queries": [
              {
                "data_source": "metrics",
                "name": "query1",
                "query": "avg:health.ci.tests_passing.fom{*}"
              }
            ],
            "response_format": "timeseries",
            "display_type": "area"
          }
        ],
        "markers": [
          {
            "label": "Target (1.0) - All tests passing",
            "value": "y = 1",
            "display_type": "ok solid"
          },
          {
            "label": "Critical (<0.3) - Tests failing",
            "value": "y = 0.3",
            "display_type": "error dashed"
