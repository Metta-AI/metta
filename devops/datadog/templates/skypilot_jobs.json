              {
                "data_source": "metrics",
                "name": "query2",
                "query": "avg:skypilot.jobs.runtime_seconds.p90{*}"
              },
              {
                "data_source": "metrics",
                "name": "query3",
                "query": "avg:skypilot.jobs.runtime_seconds.p99{*}"
