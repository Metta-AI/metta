{
  "include": [
    "**/*.py"
  ],
  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    ".venv"
  ],
  "extraPaths": [
    "./common/src",
    "./mettagrid/src",
    "./agent/src",
    "./app_backend/src"
  ],
  "reportMissingModuleSource": false,
  "pythonVersion": "3.11",
  "venvPath": ".",
  "venv": ".venv",
  "typeCheckingMode": "off",
  "reportArgumentType": "none",
  "reportReturnType": "none",
  "reportAttributeAccessIssue": "none"
}
