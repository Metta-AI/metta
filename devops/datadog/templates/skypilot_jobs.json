        "title_size": "16",
        "title_align": "left",
        "show_legend": true,
        "legend_layout": "auto",
        "legend_columns": ["avg", "min", "max", "value"],
        "type": "timeseries",
        "requests": [
          {
            "formulas": [
              {
                "formula": "query1 / 3600",
                "alias": "p50"
              },
              {
                "formula": "query2 / 3600",
                "alias": "p90"
              },
              {
                "formula": "query3 / 3600",
                "alias": "p99"
              }
            ],
            "queries": [
              {
                "data_source": "metrics",
                "name": "query1",
                "query": "avg:skypilot.jobs.runtime_seconds.p50{*}"
