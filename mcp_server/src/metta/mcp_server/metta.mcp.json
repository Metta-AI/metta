{
  "version": "0.1",
  "name": "metta",
  "description": "Model Context Protocol configuration for the Metta project.",
  "resources": {
    "app_backend_scorecard": {
      "type": "python",
      "module": "metta.app_backend.clients.scorecard_client",
      "class": "ScorecardClient",
      "default_url": "http://localhost:8000",
      "production_url": "https://api.observatory.softmax-research.net",
      "auth": {
        "machine_token_file": "~/.metta/observatory_tokens.yaml",
        "token_loader": "metta.common.util.stats_client_cfg.get_machine_token"
      }
    },
    "wandb": {
      "type": "wandb",
      "entity": "metta-research",
      "project": "metta"
    },
    "aws_s3": {
      "type": "aws-s3",
      "region": "us-east-1",
      "buckets": ["softmax-public"]
    },
    "skypilot": {
      "type": "skypilot",
      "jobs_script": "devops/skypilot/launch.py",
      "sandbox_script": "devops/skypilot/sandbox.py",
      "config_dir": "devops/skypilot/config"
    }
  }
}
